---
import BaseLayout from "./BaseLayout.astro";
import { Icon } from "astro-icon";
const { icon } = Astro.props;
import * as CONTENT from "../pages/content";
---
<BaseLayout>
    <button type="button" class="text-black  hover:text-white border bg-white hover:bg-gray-900 focus:outline-none focus:ring-4 focus:ring-gray-300 font-medium rounded-lg text-sm px-5 py-2.5 mr-2 mb-2 dark:hover:bg-black dark:focus:ring-gray-200">
        <a href="https://drive.google.com/file/d/18TGLEhWeB1ocO-X6tGMhLPLPxCzP6s9l/view?usp=sharing" target="_blank"  class="flex items-center justify-between">
            <Icon name={icon} class="w-4 h-4 mr-1 "/> {CONTENT.buttonContent}
        </a>
    </button>
</BaseLayout>
